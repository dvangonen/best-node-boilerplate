    "eslint-plugin-jest": "^28.11.0",
    "gh-pages": "^6.3.0",
    "pkgroll": "^2.10.0",
    "prettier": "^3.5.1",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1"
  }
